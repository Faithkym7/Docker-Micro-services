# Define a group of hosts
[webservers]
192.168.33.10 ansible_user=vagrant ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key

[databases]
192.168.33.11 ansible_user=vagrant ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key

# Define a group for both webservers and databases
[all_servers:children]
webservers
databases
